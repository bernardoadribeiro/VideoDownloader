{
    "builds": [{
        "src": "config/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.11"}
    }],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "config/wsgi.py"
        },
        {
            "source": "/ads.txt",
            "destination": "https://video-downloader-topaz.vercel.app/static/ads.txt"
        }
    ]
}