{
    "builds": [{
        "src": "config/wsgi.py",
        "use":"@vercel/python",
        "config": {"maxLambdaSize": "15mb", "runtime": "python3.11"}
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "config/wsgi.py"
        }
    ],
  "redirects": [
    { "source": "/ads.txt", "destination": "/static/ads.txt" }
  ]


}